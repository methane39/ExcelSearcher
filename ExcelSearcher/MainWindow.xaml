<Window x:Class="ExcelSearcher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ExcelSearcher"
        mc:Ignorable="d"
        Title="MainWindow" Height="180" Width="450" ResizeMode="NoResize">
    <Grid>
        <Button Content="In" Height="20" Width="55" Click="Button_Click" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,10,0,0"></Button>
        <Button Content="Out" Height="20" Width="55" Click="Button_Click" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,40,0,0"></Button>
    </Grid>
</Window>
